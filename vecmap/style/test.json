                        }
                    ]
                }
            ]
        },
        {
            "type": "layer",
            "minzoom": 14,
            "maxzoom": 16,
            "filter": [
                "all",
                [
                    "in",
                    "ftCode",
                    8201
                ],
                [
                    "==",
                    "snglDbl",
                    1
                ],
                [
                    "in",
                    "railState",
                    0,
                    200
                ],
                [
                    "any",
                    [
                        "==",
                        "staCode",
                        "0"
                    ],
                    [
                        "!has",
                        "staCode"
                    ]
                ],
                [
                    "any",
                    [
                        "all",
                        [
                            ">=",
                            "rtCode1",
                            "40203000000"
                        ],
                        [
                            "<",
                            "rtCode1",
                            "40204000000"
                        ]
                    ],
                    [
                        "all",
                        [
                            ">=",
                            "rtCode",
                            "40203000000"
                        ],
                        [
                            "<",
                            "rtCode",
                            "40204000000"
                        ]
                    ]
                ]
            ],
            "list": [
                {
                    "type": "line",
                    "source-layer": "railway",
                    "info": {
                        "type": "line"
                    },
                    "draw": {
                        "line-cap": "square",
                        "line-color": "rgba(100,100,100,1)",
                        "line-width": 3,
                        "line-visible": true
                    }
                },
                {
                    "type": "line",
                    "source-layer": "railway",
                    "info": {
                        "type": "line"
                    },
                    "draw": {
                        "line-cap": "butt",
                        "line-color": "rgba(100,100,100,1)",
                        "line-width": 8,
                        "line-dasharray": [
                            0.1,
                            5
                        ],
                        "line-visible": true
                    }
                }
            ],
            "visible": true
        },
